<!doctype html>
<html lang="en">

<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<!-- Bootstrap CSS -->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
	{{!-- <link href="../../style.css" rel="stylesheet" type="text/css" /> --}}
	{{!-- <link rel='stylesheet' href="../../public/index.css" /> --}}
	<title>Space Saving Furniture Systems</title>
	<link rel="icon" type="image/jpeg" href="/images/compLogo.jpg">
</head>

<body>
	<section class="relative mx-auto mb-2">
		<!-- navbar -->
		<nav class="flex items-center justify-between flex-wrap bg-gray-700 lg:px-12 shadow border-solid">
			<div class="flex justify-between lg:w-auto w-full lg:border-b-0 pr-2 pb-1 lg:pb-0">
				<div class="flex items-center flex-shrink-0 text-gray-800 mr-16">
					<span class="font-semibold text-xl tracking-tight">
						{{!-- Add logo here --}}
						<a href="/"><img
								src="https://dp.image-gmkt.com/SG/GMKT.IMG/front_image/minishop/2014/01/02/a6b99c03-175e-40eb-8edb-104c4e3c488e.s_110-w-fs-st_s.jpg"
								style="width: 120px; text-align:left; margin-left:10px; padding-top:10px; padding-bottom:10px" /></a>
						<h1 style="color:white " class='text-left'>Space Saving Furniture Systems</h1>
					</span>
				</div>
				<div class="block lg:hidden">
					<button id="nav"
						class="flex items-center px-3 py- border-2 rounded text-gray-100 border-blue-700 hover:text-gray-100 hover:border-blue-700">
						<svg class="fill-current h-3 w-3" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
							<title>Menu</title>
							<path d="M0 3h20v2H0V3zm0 6h20v2H0V9zm0 6h20v2H0v-2z"></path>
						</svg>
					</button>
				</div>
			</div>

			{{! menu }}
			<div class="menu w-full flex-grow sm:flex sm:items-center sm:w-auto sm:px-3 px-3 pb-3">
				<div class="text-md font-bold text-gray-100 lg:flex-grow d-flex justify-items-end">
					<a href="/allproducts"
						class="block mt-4 lg:inline-block lg:mt-0 px-2 py-2 mr-2 transition transform hober:fixed hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">
						Catalogue
					</a>
					<a href="/create"
						class="block mt-4 lg:inline-block lg:mt-0 px-2 py-2 mr-2 transition transform hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">
						Add Wall Bed
					</a>

					<a href="/checkout/createOrder"
						class="block mt-4 lg:inline-block lg:mt-0 px-2 py-2 mr-2 transition transform hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">
						Product Orders
					</a>

					<a href="/orders/shipping"
						class="block mt-4 lg:inline-block lg:mt-0 px-2 py-2 mr-2 transition transform hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">
						Shipping Details
					</a>

					<a href="/users/profile"
						class="block mt-4 lg:inline-block lg:mt-0 px-2 py-2 mr-2 transition transform hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">
						Admin Profile
					</a>
				</div>

				<div class="flex">
					{{#if user}}
					<div>
						<li
							class="font-sans block mt-4 lg:inline-block lg:mt-0 lg:ml-6 align-middle text-white hover:text-gray-200">
							<a href="/cart" role="button" class="relative flex">
								<svg class="flex-1 w-8 h-8 fill-current" viewbox="0 0 24 24">
									<path
										d="M17,18C15.89,18 15,18.89 15,20A2,2 0 0,0 17,22A2,2 0 0,0 19,20C19,18.89 18.1,18 17,18M1,2V4H3L6.6,11.59L5.24,14.04C5.09,14.32 5,14.65 5,15A2,2 0 0,0 7,17H19V15H7.42A0.25,0.25 0 0,1 7.17,14.75C7.17,14.7 7.18,14.66 7.2,14.63L8.1,13H15.55C16.3,13 16.96,12.58 17.3,11.97L20.88,5.5C20.95,5.34 21,5.17 21,5A1,1 0 0,0 20,4H5.21L4.27,2M7,18C5.89,18 5,18.89 5,20A2,2 0 0,0 7,22A2,2 0 0,0 9,20C9,18.89 8.1,18 7,18Z">
									</path>
								</svg>
								<span
									class="absolute right-0 top-0 rounded-full bg-red-600 w-4 h-4 top right p-0 m-0 text-white font-mono text-sm leading-tight text-center">
								</span>
							</a>
						</li>
					</div>
					<div class="block text-md px-4 py-2 my-3 text-gray-100 ml-2 font-bold lg:mt-0">
						Hi,
						{{user.username}}
					</div>
					<a href="/users/logout"
						class="block text-md px-4 rounded text-gray-100 ml-2 font-bold mt-4 lg:mt-0 transition transform hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">Logout</a>
					{{else}}
					<div
						class="block text-md px-4 py-2 rounded text-gray-100 ml-2 font-bold mt-4 lg:mt-0 transition transform hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">
						Welcome, Owner.</div>
					<a href="/users/login"
						class="block text-md px-4 py-2 rounded text-gray-100 ml-2 font-bold mt-4 lg:mt-0 transition transform hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">Login</a>
					<a href="/register"
						class="block text-md px-4 py-2 rounded text-gray-100 ml-2 font-bold mt-4 lg:mt-0 transition transform hover:-translate-y-1 hover:bg-gray-600 hover:border hover:rounded-xl motion-reduce:transition-none motion-reduce:transform-none">Register</a>

					{{/if}}
				</div>
			</div>
		</nav>
	</section>



	{{#if success_messages}}
	{{#each success_messages}}
	<div class="alert alert-success">
		{{this}}
	</div>
	{{/each}}
	{{/if}}

	{{#if error_messages}}
	{{#each error_messages}}
	<div class="alert alert-danger">
		{{this}}
	</div>
	{{/each}}
	{{/if}}

	{{#block 'content'}}{{/block}}
	</div>
	<!-- Optional JavaScript -->
	<!-- jQuery first, then Popper.js, then Bootstrap JS -->
	<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
		integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
		crossorigin="anonymous"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
		integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
		crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
		integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
		crossorigin="anonymous"></script>

	{{#block 'js'}}

	{{/block}}
</body>

